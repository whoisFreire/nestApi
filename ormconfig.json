{
    "type": "sqlite",
    "database": "./app.db",
    "cache": true,
    "entities": ["./src/entity/**/*.entity{.ts,.js}"],
    "migrations": ["./src/migration/*.ts"],
    "cli": {
        "migrationsDir": "./src/migration",
        "entitiesDir": "./src/models"
    },
    "ssl": true,
    "extra": {
      "ssl": {
        "rejectUnauthorized": false
      }
    }
}